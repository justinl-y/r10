{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins" : [
    "import",
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "global-require": 0,
    "no-case-declarations": 0,
    "no-param-reassign": 1,
    "react/prefer-stateless-function": 1,
    "react/jsx-no-bind": 0,
    "react/jsx-filename-extension": 0,
    "no-underscore-dangle": 0,
    "max-len": 1
  }
}